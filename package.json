{
    "scripts": {
        "frontend:install": "cd Syft && npm install -f",
        "frontend:start": "cd Syft && npm run start",
        "frontend:build": "cd Syft && npm run build",
        "frontend:test": "cd Syft && npm run test",
        "frontend:e2e": "cd Syft && npm run e2e",
        "frontend:lint": "cd Syft && npm run lint",
        "frontend:deploy": "cd Syft && npm run deploy",
        "api:install": "cd full-stack-syft/syft-api && npm install .",
        "api:build": "cd full-stack-syft/syft-api && npm run build",
        "api:start": "cd full-stack-syft/syft-api && npm run dev",
        "api:deploy": "cd full-stack-syft/syft-api && npm run deploy",
        "deploy": "npm run api:deploy && npm run frontend:deploy"
    }
}
